<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema
    targetNamespace="http://myservice.be/InputCatalog/v001"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:tns="http://myservice.be/InputCatalog/v001">

    <xsd:element name="flowType" type="tns:flowTypeType"/>
	<xsd:simpleType name="flowTypeType">
		<xsd:annotation>
			<xsd:documentation>Light or Full</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LIGHT" />
			<xsd:enumeration value="FULL" />
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>